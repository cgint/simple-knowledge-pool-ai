{
  "name": "simple-knowledge-pool-ai",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "p": "sh precommit.sh",
    "pdev": "npm run p && npm run dev",
    "pprev": "npm run p && npm run preview",
    "convert:mht": "node scripts/convert-mht-to-pdf.mjs",
    "convert:mht:aspose": "node scripts/convert-mht-with-aspose.mjs"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-node": "^5.2.14",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/svelte": "^5.2.8",
    "@types/node": "^24.1.0",
    "@types/sanitize-html": "^2.13.0",
    "@types/uuid": "^10.0.0",
    "jsdom": "^26.1.0",
    "svelte": "^5.0.0-next.154",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^6.0.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@google/generative-ai": "0.*",
    "eslint-plugin-unused-imports": "^4.1.4",
    "html-pdf": "^3.0.1",
    "marked": "^12.0.2",
    "mhtml-to-html": "^2.0.0",
    "sanitize-html": "^2.13.0",
    "uuid": "^11.1.0",
    "wkhtmltopdf": "^0.4.0"
  }
